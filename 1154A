#include<stdio.h>
#include<conio.h>
void main()
 {
 clrscr();
int arr[4],ar[3];
int i,temp,j,k=0;
for(i=0; i<4;i++)
scanf("%d",&arr[i]);
temp= arr[0];
 for(j=0;j<4;j++)
  {
  if (temp<arr[j])
      temp=arr[j];
  }
for(j=0;j<4;j++)
  if(arr[j]==temp);
  else
   { ar[k]=arr[j];
   k++;
   }
printf("%d %d %d",temp-ar[2],temp-ar[1],temp-ar[0]);
getch();
 }
